    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@mdjs/core": "^0.4.0",
    "@mdx-js/mdx": "^1.5.1",
    "detab": "^2.0.3",
    "mdurl": "^1.0.1",
    "remark-html": "^10.0.0",
    "remark-parse": "^7.0.2",
    "remark-slug": "^5.1.2",
    "storybook-prebuilt": "^1.5.0",
    "unified": "^8.4.2",
    "unist-builder": "^2.0.2",
    "unist-util-visit-parents": "^3.0.2"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.3",
